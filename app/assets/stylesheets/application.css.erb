/**			AdendaPro Button			**/
	.btn-agendapro-oscuro {
	  color: #fff;
	  background-color: #1B7370;
	  border-color: #186563;
	}
	.btn-agendapro-oscuro:hover,
	.btn-agendapro-oscuro:focus,
	.btn-agendapro-oscuro:active,
	.btn-agendapro-oscuro.active,
	.open > .dropdown-toggle.btn-agendapro-oscuro {
	  color: #fff;
	  background-color: #0F5250;
	  border-color: #104341;
	}
	.btn-agendapro-oscuro:active,
	.btn-agendapro-oscuro.active,
	.open > .dropdown-toggle.btn-agendapro-oscuro {
	  background-image: none;
	}
	.btn-agendapro-oscuro.disabled,
	.btn-agendapro-oscuro[disabled],
	fieldset[disabled] .btn-agendapro-oscuro,
	.btn-agendapro-oscuro.disabled:hover,
	.btn-agendapro-oscuro[disabled]:hover,
	fieldset[disabled] .btn-agendapro-oscuro:hover,
	.btn-agendapro-oscuro.disabled:focus,
	.btn-agendapro-oscuro[disabled]:focus,
	fieldset[disabled] .btn-agendapro-oscuro:focus,
	.btn-agendapro-oscuro.disabled:active,
	.btn-agendapro-oscuro[disabled]:active,
	fieldset[disabled] .btn-agendapro-oscuro:active,
	.btn-agendapro-oscuro.disabled.active,
	.btn-agendapro-oscuro[disabled].active,
	fieldset[disabled] .btn-agendapro-oscuro.active {
	  background-color: #1B7370;
	  border-color: #186563;
	}
	.btn-agendapro-oscuro {
	  text-shadow: 0 -1px 0 rgba(0, 0, 0, .2);
	  -webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, .15), 0 1px 1px rgba(0, 0, 0, .075);
	          box-shadow: inset 0 1px 0 rgba(255, 255, 255, .15), 0 1px 1px rgba(0, 0, 0, .075);
	}
	.btn-agendapro-oscuro:active,
	.btn-agendapro-oscuro.active {
	  -webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
	          box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
	}
	.btn-agendapro-oscuro {
	  background-image: -webkit-linear-gradient(top, #1B7370 0%, #0E4D4B 100%);
	  background-image:      -o-linear-gradient(top, #1B7370 0%, #0E4D4B 100%);
	  background-image: -webkit-gradient(linear, left top, left bottom, from(#1B7370), to(#0E4D4B));
	  background-image:         linear-gradient(to bottom, #1B7370 0%, #0E4D4B 100%);
	  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff428bca', endColorstr='#ff2d6ca2', GradientType=0);
	  filter: progid:DXImageTransform.Microsoft.gradient(enabled = false);
	  background-repeat: repeat-x;
	  border-color: #2b669a;
	}
	.btn-agendapro-oscuro:hover,
	.btn-agendapro-oscuro:focus {
	  background-color: #0E4D4B;
	  background-position: 0 -15px;
	}
	.btn-agendapro-oscuro:active,
	.btn-agendapro-oscuro.active {
	  background-color: #0E4D4B;
	  border-color: #2b669a;
	}
	.btn-agendapro-oscuro:disabled,
	.btn-agendapro-oscuro[disabled] {
	  background-color: #0E4D4B;
	  background-image: none;
	}

