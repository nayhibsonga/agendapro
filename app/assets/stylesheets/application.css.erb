/**			Panels			**/
	.panel > .list-group {
	  max-height: 300px;
	  overflow: auto;
	}
	.panel > div > .list-group {
	  max-height: 300px;
	  overflow: auto;
	}
	.panel-info {
		border-color: rgba(27, 115, 112, 0.6);
	}
	.panel-info>.panel-heading {
		color: rgba(27, 115, 112, 1);
		background-color: rgba(27, 115, 112, 0.4);
		border-color: rgba(27, 115, 112, 0.67);
	}
	.panel-group > .panel > .panel-heading > h3,
	.panel-group > .panel > .panel-heading > h4 {
		cursor: pointer;
	}
/**			End Panels			**/

