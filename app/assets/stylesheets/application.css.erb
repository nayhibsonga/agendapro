/*****************************
**	Seteo de Bootstrap		**
*****************************/

/**			Panels			**/
	.panel > .list-group {
	  max-height: 300px;
	  overflow: auto;
	}
	.panel > div > .list-group {
	  max-height: 300px;
	  overflow: auto;
	}
	.panel-info {
		border-color: rgba(27, 115, 112, 0.6);
	}
	.panel-info>.panel-heading {
		color: #ffffff;/*rgba(27, 115, 112, 1);*/
		border-color: rgba(27, 115, 112, 0.67);
		background-image: -webkit-linear-gradient(top, #259F9D 0%, #008881 100%);
		background-image:      -o-linear-gradient(top, #259F9D 0%, #008881 100%);
		background-image: -webkit-gradient(linear, left top, left bottom, from(#259F9D), to(#008881));
		background-image:         linear-gradient(to bottom, #259F9D 0%, #008881 100%);
		filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff259F9D', endColorstr='#ff008881', GradientType=0);
		background-repeat: repeat-x;
	}
	.panel-title {
		overflow: hidden;
		text-overflow: ellipsis;
		white-space: nowrap;
	}
	.panel-title > input[type="checkbox"] {
		margin-right: 5px;
	}
	.panel-group > .panel > .panel-heading > .panel-title {
		cursor: pointer;
	}
	.panel-group > .panel > .panel-heading > .panel-title > a:after,
	.panel-group > .panel > .panel-heading > .panel-title.collapsed > a:after,
	.panel-group > .panel > .panel-heading > .panel-title > a.collapsed:after {
		float: right !important;
		/* Font awesome style */
		font-family: 'FontAwesome';
		src: url(<%= font_path('fontawesome-webfont.eot?v=4.1.0') %>);
		src: url(<%= font_path('fontawesome-webfont.eot?#iefix&v=4.1.0') %>) format('embedded-opentype'), url(<%= font_path('fontawesome-webfont.woff?v=4.1.0') %>) format('woff'), url(<%= font_path('fontawesome-webfont.ttf?v=4.1.0') %>) format('truetype'), url(<%= font_path('fontawesome-webfont.svg?v=4.1.0#fontawesomeregular') %>) format('svg');
		font-weight: normal;
		font-style: normal;
		display: inline-block;
		font-family: FontAwesome;
		font-style: normal;
		font-weight: normal;
		line-height: 1;
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale;
	}
	.panel-group > .panel > .panel-heading > .panel-title > a:after {
		/* Font symbol */
		content: "\f0d7";
	}
	.panel-group > .panel > .panel-heading > .panel-title.collapsed > a:after,
	.panel-group > .panel > .panel-heading > .panel-title > a.collapsed:after {
		/* Font symbol */
		content: "\f0d9";
	}
/**			End Panels			**/

