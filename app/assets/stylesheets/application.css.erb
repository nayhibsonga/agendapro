/**			Modals			**/
	.modal-content{
		background: #ececec;
		background: -moz-linear-gradient(top,  #ececec 0%, #f0f6f6 100%);
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ececec), color-stop(100%,#f0f6f6));
		background: -webkit-linear-gradient(top,  #ececec 0%,#f0f6f6 100%);
		background: -o-linear-gradient(top,  #ececec 0%,#f0f6f6 100%);
		background: -ms-linear-gradient(top,  #ececec 0%,#f0f6f6 100%);
		background: linear-gradient(to bottom,  #ececec 0%,#f0f6f6 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ececec', endColorstr='#f0f6f6',GradientType=0 );
	}
	.modal-header{
	    background: #dddfde;
		background: -moz-linear-gradient(top,  #dddfde 0%, #cbcdcc 100%);
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#dddfde), color-stop(100%,#cbcdcc));
		background: -webkit-linear-gradient(top,  #dddfde 0%,#cbcdcc 100%);
		background: -o-linear-gradient(top,  #dddfde 0%,#cbcdcc 100%);
		background: -ms-linear-gradient(top,  #dddfde 0%,#cbcdcc 100%);
		background: linear-gradient(to bottom,  #dddfde 0%,#cbcdcc 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#dddfde', endColorstr='#cbcdcc',GradientType=0 );
	}
	.modal-footer{
		background: #cbcdcc;
		background: -moz-linear-gradient(top,  #cbcdcc 0%, #dddfde 100%);
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#cbcdcc), color-stop(100%,#dddfde));
		background: -webkit-linear-gradient(top,  #cbcdcc 0%,#dddfde 100%);
		background: -o-linear-gradient(top,  #cbcdcc 0%,#dddfde 100%);
		background: -ms-linear-gradient(top,  #cbcdcc 0%,#dddfde 100%);
		background: linear-gradient(to bottom,  #cbcdcc 0%,#dddfde 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#cbcdcc', endColorstr='#dddfde',GradientType=0 );
	}
/**			End Modals			**/

