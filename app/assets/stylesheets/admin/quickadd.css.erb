/*
  ================================
  ============ Start =============
  ================================
*/
.content-fill {
  height: calc(100% - 100px);
  min-height: calc(100% - 100px);
  padding-bottom: 50px;
  padding-top: 50px;
  position: relative;
}
.background-start {
  background-image: url(<%= asset_path 'quickadd/start.png' %>);
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
  color: #ffffff;
}
.vertical-center {
  height: 300px;
  bottom: 0;
  left: 0;
  margin: auto;
  position: absolute;
  right: 0;
  top: 0;
}
.content-top {
  position: absolute;
  top: 50px;
}
.content-bottom {
  bottom: 50px;
  position: absolute;
}
.btn-wide {
  border-radius: 10px;
  font-size: 25px;
  padding: 15px 35px;
}

section.green-background {
  background-color: #22c488;
  color: #ffffff;
  padding-bottom: 20px;
  margin-bottom: 20px;
}

th {
  text-align: center;
}

form.form-inline {
  margin-top: 10px;
  margin-bottom: 20px;
}

div.category-pill {
  color: #22c488;
  background-color: #ffffff;
  padding: 10px;
  margin: 5px;
  border-radius: 4px;
  display: inline-block;
}

button.service-category-delete-btn {
  color: #22c488;
  border: 0;
}

div.step5-check {
  font-size: 72px;
}

h3.step5-text {
  padding-left: 15%;
  padding-right: 15%;
}

a.btn-green-hover:hover {
  background-color: #22c488;
  color: #ffffff;
}

a.btn-orange-hover:hover {
  background-color: #fd9610;
  color: #ffffff;
}

.form-inline > .form-group {
  margin-right: 50px;
}

.next-button {
  margin-top: 30px;
  padding: 15px 35px;
}


/*
  ================================
  =========== Company ============
  ================================
*/
.label-green {
  color: #22c488;
  font-size: 30px;
  margin-bottom: 10px;
  margin-top: 20px;
  font-weight: 500;
  line-height: 1.1;
}
.quickadd-logo-input {
  display: none;
}
input[type="file"] {
  display: none;
}
.quickadd-logo-label {
  border-radius: 4px;
  border: 1px solid #ccc;
  display: inline-block;
  padding: 6px 12px;
  cursor: pointer;
  color: #ffffff;
  background-color: #22c488;
}
.quickadd-logo-label:hover {
  background-color: #1ba970;
}
.company-logo {
  border-radius: 4px;
  border: 1px solid #ccc;
  overflow: hidden;
  margin-bottom: 10px;
}

/*
  ================================
  ========== New Local ===========
  ================================
*/
.background-green {
  background: #22c488;
  color: #ffffff;
}
.text-green {
  color: #22c488;
}
.text-dark-green {
  color: #4d685c;
}
.quickadd-pills {
  margin-bottom: 10px;
}
.quickadd-pills > li > a {
  background-color: #ffffff;
  color: #22c488;
  margin-right: 5px;
}
.quickadd-pills > li.active > a {
  background-color: #fd9610;
  color: #ffffff;
}
.quickadd-pills > li.active > a:hover {
  background-color: #db7400;
  color: #ffffff;
}
.quickadd-pills > li.active > a:focus {
  background-color: #db7400;
  color: #ffffff;
}

.panel-info {
  border-color: #505050;
}
table.table tbody#localTable th, .table tbody#localTable td {
  padding: 0;
  border-top: none !important;
  margin-bottom: 0;
}
table.table tbody#localTable th form, .table tbody#localTable td form {
  margin-bottom: 0;
}

#localTable .form-inline > .form-group {
  margin-right: 0;
}

#localTable > tr > td:nth-child(2) {
  text-align: right;
}