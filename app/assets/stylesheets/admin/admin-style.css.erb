/*
  ================================
  ========== Scroll Bar ==========
  ================================
*/
::-webkit-scrollbar {
  width: 10px;
}
::-webkit-scrollbar-track {
    background-color: #eaeaea;
    border-left: 1px solid #ccc;
}
::-webkit-scrollbar-thumb {
    background-color: #ccc;
}
::-webkit-scrollbar-thumb:hover {
    background-color: #aaa;
}

/*
  ================================
  =========== General ============
  ================================
*/
@font-face {
  font-family: TradeGothic;
  src: url('/assets/Trade Gothic LT Condensed No. 18.ttf');
  src: url('/assets/Trade Gothic LT Condensed No. 18.ttf?#iefix') format('truetype'),
  url('/assets/Trade Gothic LT Condensed No. 18.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: FranklinGothic;
  src: url('/assets/ITC Franklin Gothic LT Book Extra Compressed.ttf');
  src: url('/assets/ITC Franklin Gothic LT Book Extra Compressed.ttf?#iefix') format('truetype'),
  url('/assets/ITC Franklin Gothic LT Book Extra Compressed.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

html,
body {
  font-family: TradeGothic, FranklinGothic, "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
  background: #ffffff;
  font-size: 17px;
  color: #505050;
}
body {
  padding-top: 50px;
}
.pull-center {
  text-align: center;
  display: block;
  margin-left: auto;
  margin-right: auto;
}
a {
  color: #22c488;
  text-decoration: none;
}
a:hover,
a:active {
  color: #1ba970;
}
.alert {
  margin-top: 5px;
}

