/*
  ================================
  ========= Navbar Admin =========
  ================================
*/
.navbar-admin {
  background-color: #ffffff;
  border-color: #e7e7e7;
  -webkit-transition: all 0.5s ease-in-out;
  -moz-transition: all 0.5s ease-in-out;
  -o-transition: all 0.5s ease-in-out;
  transition: all 0.5s ease-in-out;
}
.navbar-admin > .container .navbar-brand {
  margin-left: 0px;
}
.navbar-admin .navbar-nav > li > a,
.navbar-admin .navbar-nav > li > a:focus {
  color: #646464;
  text-transform: uppercase;
  font-family: TradeGothic, FranklinGothic, "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
  font-size: 16px;
}
.navbar-admin .navbar-nav > li > a:hover {
  color: #cccccc;
  background-color: transparent;
}
.navbar-admin .navbar-nav > .active > a {
  color: #a5a5a5;
  background-color: transparent;
}
.navbar-admin .navbar-nav > .active > a:hover,
.navbar-admin .navbar-nav > .active > a:focus {
  color: #cccccc;
  background-color: transparent;
}
.navbar-admin .navbar-nav > li > a.menu-green {
  color: #22c488;
}
.navbar-admin .navbar-nav > li > a.menu-green:hover {
  color: #fd9610;
}
.navbar-brand {
  padding: 5px 5px;
}
.navbar-admin .navbar-toggle {
  margin-top: 4px;
  margin-bottom: 4px;
}
.navbar-admin .navbar-nav .open .dropdown-menu > li > a {
  color: #646464;
}
.navbar-admin .navbar-nav .open .dropdown-menu > li > a:hover,
.navbar-admin .navbar-nav .open .dropdown-menu > li > a:focus {
  color: #cccccc;
  background-color: transparent;
}
.navbar-admin .navbar-nav .open .dropdown-menu > .active > a,
.navbar-admin .navbar-nav .open .dropdown-menu > .active > a:hover,
.navbar-admin .navbar-nav .open .dropdown-menu > .active > a:focus {
  color: #a5a5a5;
  background-color: transparent;
}
.navbar-default .navbar-nav > .open > a,
.navbar-default .navbar-nav > .open > a:hover,
.navbar-default .navbar-nav > .open > a:focus {
  color: #a5a5a5;
  background-color: transparent;
}

/*
  ================================
  ========== Datepicker ==========
  ================================
*/
.ui-widget {
  font-size: inherit;
}
.ui-datepicker {
  width: 14em;
}
.ui-datepicker .ui-datepicker-prev span,
.ui-datepicker .ui-datepicker-next span {
  margin-top: -9px;
}
.ui-datepicker .ui-datepicker-title {
  line-height: inherit;
}
.ui-datepicker table {
  font-size: .8em;
}
.ui-datepicker th {
  padding: .5em .3em;
}
.ui-datepicker td span,
.ui-datepicker td a {
  padding: .1em;
  text-align: center;
}

/*
  ================================
  ========== Dashboard ===========
  ================================
*/
.panel-green {
  border-color: #1ba970;
}
.panel-green > .panel-heading {
  color: #505050;
  background-color: #22c488;
  border-color: #1ba970;
}
.panel-green > .panel-heading + .panel-collapse > .panel-body {
  border-top-color: #1ba970;
}
.panel-green > .panel-heading .badge {
  color: #22c488;
  background-color: #505050;
}
.panel-green > .panel-footer + .panel-collapse > .panel-body {
  border-bottom-color: #1ba970;
}
.panel-scrollable {
  max-height: 337px;
  overflow-y: auto;
  overflow-y: overlay;
}
.label-agendapro {
  background-color: #22c488;
}

/*
  ================================
  =========== Calendar ===========
  ================================
*/
.background-grey {
  background-color: #f5f5f5;
  border: 1px solid #e3e3e3;
  padding: 10px 15px;
}

/*
  ================================
  =========== Clients ============
  ================================
*/
.text-green {
  color: #22c488;
}
.table caption {
  caption-side: bottom;
  text-align: right;
  background-color: #f5f5f5;
  padding: 4px 8px;
}
.table-green > thead > tr > th,
.table-green > thead > tr > td {
  background-color: #22c488;
  color: #ffffff;
  border-right: transparent;
  border-left: transparent;
}
.list-separate > li:not(:last-child) {
  margin-bottom: inherit;
}

/*
  ================================
  =========== Reports ============
  ================================
*/
.table-summary > tfoot > tr > td {
  font-weight: bold;
}

/*
  ================================
  ========== Locations ===========
  ================================
*/
#map {
  width: 100%;
  height: 260px;
}
#error {
  font-weight: bold;
  color: #fd633f;
}
.text-buttons {
  margin-top: 10px;
}
.action-buttons {
  margin-bottom: 10px;
  text-align: right;
}
@media (max-width: 992px) {
  .action-buttons {
    text-align: initial;
  }
  .action-buttons > a:last-child {
    float: right;
  }
}

/*
  ================================
  ========== Resources ===========
  ================================
*/
.checkbox-text {
  vertical-align: inherit;
  max-width: 209px;
}

/*
  ================================
  ====== Company Settings ========
  ================================
*/
.tab-margin {
  margin-top: 20px;
}
.tab-margin.nav-pills > li > a,
.tab-margin.nav-pills > li > a:hover,
.tab-margin.nav-pills > li > a:focus {
  color: #505050;
}
.tab-margin.nav-pills > li.active > a,
.tab-margin.nav-pills > li.active > a:hover,
.tab-margin.nav-pills > li.active > a:focus {
  background-color: #f5f5f5;
  border: 1px solid #e3e3e3;
  color: #505050;
}
.tab-margin .media {
  margin-bottom: 15px;
  min-height: 100px;
}
.left-panel {
  border-left: 1px solid #e3e3e3;
}
.media-image {
  border: solid 1px #22c488;
  border-radius: 10px;
  padding: 5px;
  width: 50px;
  height: 50px;
}
.company-form label {
  color: #22c488;
}
.company-logo {
  border-radius: 10px;
  border: 1px solid #ccc;
  padding: 10px;
  width: 100px;
  height: 100px;
}
.background-green {
  background-color: #22c488;
  padding: 12px 24px;
  color: #ffffff;
  border-radius: 6px;
}
.background-green label {
  color: #ffffff;
}
.company-form .help-block {
  margin-top: 0;
}
.company-form .checkbox > span.help-block {
  margin-left: 20px;
}
.circle-list {
  margin-top: 15px;
  margin-bottom: 5px;
}
.circle-list > span {
  display: inline-block;
  padding: 5px 10px;
  font-size: 20px;
  font-weight: bold;
  line-height: 1;
  color: #ffffff;
  text-align: center;
  white-space: nowrap;
  vertical-align: middle;
  background-color: #22c488;
  border-color: #1ba970;
  border-radius: 50%;
}
.circle-list > p {
  display: inline-block;
  border-left: 1px solid #aaa;
  padding-left: 5px;
}
.select-modification {
  display: inline-block;
  width: calc(100% / 2 - 2px);
}
