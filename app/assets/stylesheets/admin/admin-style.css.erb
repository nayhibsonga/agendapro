/* Se agrega estilo de títulos y textos varios */
.hgroup {
  padding-top: 26px;
  /*padding-bottom: 26px;*/
}
.hgroup .container h1 {
  color: #006D68;
}
.hgroup .container h1 {
  font-size: 50px;
  font-weight: 500;
  line-height: 1em;
  margin: 0;
  padding: 0;
}
.hgroup h2 {
  font-size: 35px;
  font-weight: 300;
  line-height: 1.5em;
  margin: 0 20% 0 0;
  padding: 0;
  color: #505050;
}
h2.section_header {
  font-size: 18px;
  line-height: 22px;
  font-weight: 400;
}
h3.link_section_header {
  font-size: 24px;
  line-height: 22px;
  font-weight: 400;
  margin-top: 5px;
  color: #505050;
}
.hgroup .breadcrumb {
  font-size: 12px;
  background-color: #f3f3f3;
  padding: 6px 15px;
  border: solid 1px #cccccc;
}
.line-breadcrumb{
  height: 30px;
  width: 100%;
  background-image: url(<%= asset_path 'home/features/line.png' %>);
  margin-top: 5px;
}
.breadcrumb>.active {
  color: #999;
}

/* Se agrega para cada paso del QuickAdd */
.step-workflow{
  min-height: 590px;
}

.step-workflow-min{
  min-height: 280px;
}

/* Se agrega para achicar los selectores de las horas que estaban muy grandes. Quedan para pantallas de 1280px */
.time-select{
  padding-left: 4px;
  padding-right: 4px;
}

.date-select{
  padding-left: 6px;
  padding-right: 6px;
  width: auto;
  display: inline;
}

#wrap-admin{
  min-height: 100%;
  height: auto;
  margin: 0 auto -40px;
  padding: 0 0 40px;
}
.fix-height{
  min-height: 100%;
}

/* Agregado 18 Febrero */
.caret-especial{
  float: right;
  margin-right: 10px;
  font-size: 21px;
}

.fc-event-vert .fc-event-bg {
  display: none;
}

/* Quick Add */
#navegacion-workflow{
  margin:10px 0px 0px 0px;
  background: #F3F3F3;
  border: 1px solid #ccc;
  border-radius: 5px;
  padding:5px 0px;
}
.pasos-reserva a{
  height:33px;
  width: 30%;
  margin:0px 1.5%;
  float:left;
  text-align: center;
  background: #D1D1D1;
  bordert:1px solid #999;
  color: #fff;
  line-height: 2.4;
  border-radius: 5px;
  border:1px solid #585656;
}
.pasos-reserva a:hover, .pasos-reserva a.selected{
  background-color: rgba(250, 70, 22, 0.45);
  border:1px solid #FA4616;
}
#espacio-workflow{
  margin-top:10px;
}

.box-step{
  /*min-height:350px;*/
  background:#F3F3F3;
  border: 1px solid #C0C0C0;
  border-radius: 5px;
  float:left;
}
#control-slide li{
  display: inline-block;
}
.control-box{
  background-color: #ccc;
  display: block;
  padding: 6px 0px;
  margin-bottom: 0;
  font-size: 14px;
  font-weight: normal;
  line-height: 1.428571429;
  text-align: center;
  white-space: nowrap;
  vertical-align: middle;
  cursor: pointer;
  background-image: none;
  border: 1px solid transparent;
  border-radius: 4px;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  -o-user-select: none;
  user-select: none;
}
li.selected> div.control-box{
  background: rgba(27,115,112,0.4);
}

/* Agregar margen a las filas, usado en vista cliente */
.top-buffer {
  margin-top:20px;
}

/* Paginación */

.fixed-panel{
    max-height: 250px;
    overflow-y:scroll;
}
.fixed-table{
    max-height: 350px;
    overflow-y:scroll;
}
.table-checkboxes label
{
    max-width: 90%;
}

p.justified {
  text-align:justify;
}

.panel > .panel-body.payments-panel {
