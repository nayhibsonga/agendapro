/*
  ================================
  ========= Navbar Admin =========
  ================================
*/
.navbar-admin {
  background-color: #ffffff;
  border-color: #e7e7e7;
  -webkit-transition: all 0.5s ease-in-out;
  -moz-transition: all 0.5s ease-in-out;
  -o-transition: all 0.5s ease-in-out;
  transition: all 0.5s ease-in-out;
}
.navbar-admin > .container .navbar-brand {
  margin-left: 0px;
}
.navbar-admin .navbar-nav > li > a,
.navbar-admin .navbar-nav > li > a:focus {
  color: #646464;
  text-transform: uppercase;
  font-family: 'Roboto Condensed', sans-serif;
  font-size: 16px;
}
.navbar-admin .navbar-nav > li > a:hover {
  color: #cccccc;
  background-color: transparent;
}
.navbar-admin .navbar-nav > .active > a {
  color: #a5a5a5;
  background-color: transparent;
  text-decoration: underline;
}
.navbar-admin .navbar-nav > .active > a:hover,
.navbar-admin .navbar-nav > .active > a:focus {
  color: #cccccc;
  background-color: transparent;
}
.navbar-admin .navbar-nav > li > a.menu-green {
  color: #22c488;
}
.navbar-admin .navbar-nav > li > a.menu-green:hover {
  color: #fd9610;
}
.navbar-brand {
  padding: 5px 5px;
}
.navbar-admin .navbar-toggle {
  margin-top: 4px;
  margin-bottom: 4px;
}
.navbar-admin .navbar-nav .open .dropdown-menu > li > a {
  color: #646464;
}
.navbar-admin .navbar-nav .open .dropdown-menu > li > a:hover,
.navbar-admin .navbar-nav .open .dropdown-menu > li > a:focus {
  color: #cccccc;
  background-color: transparent;
}
.navbar-admin .navbar-nav .open .dropdown-menu > .active > a,
.navbar-admin .navbar-nav .open .dropdown-menu > .active > a:hover,
.navbar-admin .navbar-nav .open .dropdown-menu > .active > a:focus {
  color: #a5a5a5;
  background-color: transparent;
}
.navbar-admin .navbar-nav > .open > a,
.navbar-admin .navbar-nav > .open > a:hover,
.navbar-admin .navbar-nav > .open > a:focus {
  color: #a5a5a5;
  background-color: transparent;
}
.email-dropdown {
  text-overflow: clip;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  max-width: 220px;
  display: inline-block;
}
.email-dropdown + i {
  position: relative;
  top: -5px;
}
@media (min-width: 768px) {
  .navbar-admin .navbar-nav > li > a,
  .navbar-admin .navbar-nav > li > a:focus {
    font-size: 12px;
  }
  .nav > li > a {
    padding: 10px 6px;
  }
  .dropdown-menu > li > a {
    padding: 3px 10px;
    font-size: 12px;
  }
  .email-dropdown {
    max-width: 40px;
  }
}
@media (min-width: 992px) {
  .navbar-admin .navbar-nav > li > a,
  .navbar-admin .navbar-nav > li > a:focus {
    font-size: 14px;
  }
  .nav > li > a {
    padding: 10px 10px;
  }
  .dropdown-menu > li > a {
    padding: 3px 15px;
    font-size: 13px;
  }
  .email-dropdown {
    max-width: 120px;
  }
}
@media (min-width: 1200px) {
  .navbar-admin .navbar-nav > li > a,
  .navbar-admin .navbar-nav > li > a:focus {
    font-size: initial;
  }
  .nav > li > a {
    padding: 10px 15px;
  }
  .dropdown-menu > li > a {
    padding: 3px 20px;
    font-size: inherit;
  }
  .email-dropdown {
    max-width: 150px;
  }
}

