}

/*
  ================================
  ========== Dashboard ===========
  ================================
*/
.rec-highlight {
  background-color: #f5f5f5;
  padding: 5px 7px;
  font-weight: 600;
}
.square-highlight,
.square-green {
  background-color: #f5f5f5;
  padding: 5px;
  text-align: center;
  font-weight: 600;
  height: auto;
}
.square-green {
  background-color: #22c488;
  color: #ffffff;
}
.square-highlight > h2,
.square-green > h2 {
  /*font-size: 3.5em;*/
  margin-top: 10px;
  font-weight: bold;
}
.panel-body-grey {
  background-color: #f5f5f5;
}
@media(max-width: 767px) {
  .rec-highlight {
    text-align: center;
  }
}
@media(max-width: 991px) {
  .rec-highlight,
  .square-highlight,
  .square-green {
    border: solid #e3e3e3 1px;
    margin-bottom: 5px;
  }
  .square-green {
    border-color: #1ba970;
  }
}
@media(min-width: 768px) {
  .rec-highlight {
    padding-left: 15%;
  }
  .square-highlight > h2,
  .square-green > h2 {
    font-size: 1.52em;
  }
}
@media(min-width: 992px) {
  .rec-highlight + .rec-highlight {
    margin-top: 3px;
  }
  .square-highlight,
  .square-green {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
        -ms-flex-pack: center;
            justify-content: center;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
        -ms-flex-direction: column;
            flex-direction: column;
    height: 219px;
    padding: 10px 5px;
  }
  .square-highlight > h2,
  .square-green > h2 {
    margin-top: 20px;
  }
  .square-green > h2 {
    font-size: 1.72em;
  }
}
@media(min-width: 1200px) {
  .rec-highlight {
    padding-left: 22%;
  }
  .square-highlight > h2,
  .square-green > h2 {
    font-size: 1.72em;
  }
  .square-green > h2 {
    font-size: 2.47em;
  }
