}
.hgroup .container h1 {
  color: #006D68;
}
.hgroup .container h1 {
  font-size: 50px;
  font-weight: 500;
  line-height: 1em;
  margin: 0;
  padding: 0;
}
.hgroup h2 {
  font-size: 35px;
