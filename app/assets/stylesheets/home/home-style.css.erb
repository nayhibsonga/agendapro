/*** navbar-agendapro ***/
.navbar-agendapro {
  background-color: rgba(255,255,255,0);
  border-color: rgba(255,255,255,0);
  -webkit-transition: all 0.5s ease-in-out;
  -moz-transition: all 0.5s ease-in-out;
  -o-transition: all 0.5s ease-in-out;
  transition: all 0.5s ease-in-out;
}
.navbar-agendapro > .container .navbar-brand {
	margin-left: 0px;
}
.navbar-agendapro .navbar-nav {
  padding-top: 10px;
}
.navbar-agendapro .navbar-text {
  color: #ffffff;
}
.navbar-agendapro .navbar-nav > li {
	margin-right: 5px;
	margin-left: 5px;
}
.navbar-agendapro .navbar-nav > li > a,
.navbar-agendapro .navbar-nav > li > a:focus {
	color: #cfcdcd;
	text-transform: uppercase;
	font-family: TradeGothic, FranklinGothic, "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
	font-size: 16px;
	margin-right: 5px;
	margin-left: 5px;
}
.navbar-agendapro .navbar-nav > li > a:hover {
  color: #ffffff;
  background-color: transparent;
}
.navbar-agendapro .navbar-nav > .active > a {
  color: #ffffff;
  background-color: transparent;
}
.navbar-agendapro .navbar-nav > .active > a:hover,
.navbar-agendapro .navbar-nav > .active > a:focus {
  color: #ffffff;
  background-color: transparent;
}
.navbar-agendapro .navbar-toggle {
  border-color: #ffffff;
}
.navbar-agendapro .navbar-toggle:hover,
.navbar-agendapro .navbar-toggle:focus {
  background-color: transparent;
}
.navbar-agendapro .navbar-nav > .open > a,
.navbar-agendapro .navbar-nav > .open > a:hover {
  color: #555;
  background-color: #e7e7e7;
}
.navbar-brand {
	padding: 5px 5px;
}
.navbar-agendapro .navbar-nav > li > a.menu-green {
	color: #22c488;
	font-size: 16px;
}
.navbar-agendapro .navbar-nav > li > a.menu-green:hover {
  color: #fd9610;
}
.navbar-agendapro .navbar-nav > li > a.menu-orange {
	color: #fd9610;
	font-size: 14px;
	border: 2px solid #fd9610;
	border-radius: 15px;
	padding: 4px 14px;
	margin-top: 7px;
}
.navbar-agendapro .navbar-nav > li > a.menu-orange:hover {
  color: #ffffff;
  background-color: #fd9610;
}

