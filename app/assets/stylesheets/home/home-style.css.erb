/* Media queries */
@media (max-width: 1199px) {
    .boxed-feature > div > img {
		margin-top: 20px;
	}
	.sector-button {
		font-size: 15px;
	}
	.nav li a {
		padding: 12px 0px;
	}
	.navbar-form {
		width: 180px;
	}

	.sector_search {
		width: 16%;
		margin-right: 1.8%;
		margin-left: 1.8%;
	}
}

@media (max-width: 991px) {
	#barra-superior{
		height: auto;
	}
	#menu-display{
		display: inline-block;
		border-radius: 5px;
		border: 1px solid #fff;
		float: right;
		color: #fff;
		padding: 5px 15px;
		margin-top: 5px;
	}
	#menu-display:hover{
		background: rgba(255,255,255,0.2);
		text-decoration: none;
	}
	.nav-pills{
		display: none;
		width: 100%;
	}
	.nav-pills>li{
		float: none;
	}
	.nav-pills>li+li{
		margin-left: 0px;
	}
	.nav-pills>li>a {
		display: block;
	}
	.navbar-header {
        float: none;
    }
    .navbar-toggle {
        display: block;
    }
    .navbar-agendapro .navbar-toggle {
        display: block;
        color: #ffffff;
        border-color: #ffffff;
    }
    .navbar-collapse {
        border-top: 1px solid transparent;
        box-shadow: inset 0 1px 0 rgba(255,255,255,0.1);
    }
    .navbar-collapse.collapse {
        display: none!important;
    }
    .navbar-nav {
        float: none!important;
        /*margin: 7.5px -15px;*/
    }
    .navbar-nav>li {
        float: none;
    }
    .navbar-nav>li>a {
        padding-top: 10px;
        padding-bottom: 10px;
    }
    /* since 3.1.0 */
    .navbar-collapse.collapse.in {
        display: block!important;
    }
    .navbar-agendapro .navbar-collapse.collapse.in {
    	background-color: #646464;
    }
    .navbar-agendapro .navbar-collapse.collapsing {
    	background-color: #646464;
    }
    .collapsing {
        overflow: hidden!important;
    }
	div.pricing_plan.special {
		border: 2px solid #ffffff;
		margin: 14px;
		padding: 0;
	}
	.sector-button {
		font-size: 11px;
	}
	.sector_search {
		width: 16%;
		margin-right: 1.7%;
		margin-left: 1.7%;
		overflow: hidden;
	}
	.sector_search p {
		font-size: 11px;
	}
}

/* Small devices (tablets, 768px and up) */
@media (max-width: 767px) {
	#logo{
		margin-left: 20px;
	}
	#menu-display{
		margin-right: 20px;
	}
	.carousel-inner{
		height: 400px;
	}
	.carousel-caption{
		bottom: 120px;
	}
	.green-caption{
		width: 80%;
	}
	.green-caption p{
