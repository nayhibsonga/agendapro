.contact-footer {
	padding-top: 50px;
	padding-bottom: 10px;
	position:relative;
}
.contact-footer:before {
    content: "";

    /* Expand element */
    position: absolute;
    left: 0px;
    top: 0px;
    right: 0px;
    bottom: 0px;

    -moz-box-shadow: inset 0 30px 10px -20px rgba(0,0,0,.15);
    -webkit-box-shadow: inset 0 30px 10px -20px rgba(0,0,0,.15);
    box-shadow: inset 0 30px 10px -20px rgba(0,0,0,.15);

    /* Disable click events */
    pointer-events: none;
}
.contact-footer img,
.contact-footer p {
	margin: 5px;
	white-space: pre-line;
}
