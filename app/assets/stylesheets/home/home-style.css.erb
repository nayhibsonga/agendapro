/* Small devices (tablets, 768px and up) */
@media (max-width: 1024px) { 
	#barra-superior{
		height: auto;
	}
	#menu-display{
		display: inline-block;
		border-radius: 5px;
		border: 1px solid #fff;
		float: right;
		color: #fff;
		padding: 5px 15px;
		margin-top: 5px;
	}
	#menu-display:hover{
		background: rgba(255,255,255,0.2);
		text-decoration: none;
	}
	.nav-pills{
		display: none;
		width: 100%;
	}
	.nav-pills>li{
		float: none;
	}
	.nav-pills>li+li{
		margin-left: 0px;
	}
	.nav-pills>li>a {
		display: block;
	}
	.nav > li > a {
		padding: 12px 10px;
	}

}

/* Small devices (tablets, 768px and up) */
@media (max-width: 768px) { 
	#logo{
		margin-left: 20px;
	}
	#menu-display{
		margin-right: 20px;
	}
	.carousel-inner{
		height: 400px;
	}
	.carousel-caption{
		bottom: 120px;
	}
	.green-caption{
		width: 80%;
	}
	.green-caption p{
		font-size: 15px;
	}
}

@media (max-width: 750px) { 
	.icono p{
		display:none;
	}
}

