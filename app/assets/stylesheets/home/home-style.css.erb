.pricing_wrapper:before {
    content: "";

    /* Expand element */
    position: absolute;
    left: 0px;
    top: 0px;
    right: 0px;
    bottom: 0px;

    -moz-box-shadow: inset 0 30px 10px -20px rgba(0,0,0,1);
    -webkit-box-shadow: inset 0 30px 10px -20px rgba(0,0,0,1);
    box-shadow: inset 0 30px 10px -20px rgba(0,0,0,1);

    /* Disable click events */
    pointer-events: none;
}
