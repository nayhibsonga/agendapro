/*** navbar-agendapro ***/
.navbar-agendapro {
	background-color: rgba(255,255,255,0);
	border-color: rgba(255,255,255,0);
	-webkit-transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	transition: all 0.5s ease-in-out;
}
.navbar-agendapro > .container .navbar-brand {
	margin-left: 0px;
	padding: 5px 5px;
}
.navbar-agendapro .navbar-text {
	color: #ffffff;
}
.navbar-agendapro .navbar-toggle {
	border-color: #ffffff;
	color: #ffffff;
	padding: 5px 8px;
}
.navbar-agendapro-white {
	background-color: #ffffff;
	border-color: #eeeeee;
	-webkit-transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	transition: all 0.5s ease-in-out;
}
.navbar-agendapro-white .navbar-toggle {
	border-color: #ccc;
	color: #ccc;
}

/*background*/
.background {
	background-repeat: no-repeat;
	background-size: cover;
	background-position: center center;
	color: #ffffff;
}
.background-repeat {
	background-repeat: repeat;
	background-size: inherit;
}
.background-grey {
	background-color: #949292;
}
.background-light-grey {
	background-color: #eeeeee;
}
.background-green {
	background-color: #22c488;
	padding: 20px 0 10px;
}
.below-navbar {
	padding-top: 60px;
}
.search-text {
	color: #ffffff;
	text-transform: uppercase;
	font-size: 2.10em;
	font-family: TradeGothicCn18;
	font-weight: 300;
}
.search-text-green {
	color: #22c488;
}

/*Search Form*/
.fill { 
    min-height: 100%;
    height: 100%;
}
.container-search {
    display: table;
    vertical-align: middle;
}
.vertical-center-row {
    display: table-cell;
    vertical-align: middle;
}
.image-pin {
	width: 22.07%;
	width:	20vw;
	max-width: 99px;
}
#search-form button,
#search-form input {
	font-size: 14px;
}
.btn-search {
  color: #ffffff;
  background-color: #22c488;
  border-color: #ffffff;
}
.btn-search:hover,
.btn-search:focus,
.btn-search:active,
.btn-search.active {
  color: #818181;
  background-color: #ffffff;
  border-color: #adadad;
}
.btn-search:active,
.btn-search.active {
  background-image: none;
}
.btn-green {
	color: #ffffff;
	background-color: #22c488;
	border-color: #1ba970;
}
.btn-green:hover,
.btn-green:focus,
.btn-green.focus,
.btn-green:active,
.btn-green.active {
  color: #ffffff;
  background-color: #1ba970;
  border-color: #00CA8A;
}

.form-transparent {
	background:rgba(255,255,255,0);
    border-color: rgba(255,255,255,1);
    color: #ffffff;
}
.form-transparent:-moz-placeholder {
  color: #ffffff;
}
.form-transparent::-moz-placeholder {
  color: #ffffff;
}
.form-transparent:-ms-input-placeholder {
  color: #ffffff;
}
.form-transparent::-webkit-input-placeholder {
  color: #ffffff;
}
.green-placeholder:-moz-placeholder {
  color: #818181;
}
.green-placeholder::-moz-placeholder {
  color: #818181;
}
.green-placeholder:-ms-input-placeholder {
  color: #818181;
}
.green-placeholder::-webkit-input-placeholder {
  color: #818181;
}

/*Waht we do*/
.box-content {
	text-align: center;
	display: inline-block;
	padding: 17px 30px;
	width: 80%;
	border: 1px solid #ffffff;
	margin-bottom: 10px;
}
.box-content > p {
	margin: 9px 0 0;
}
@media (min-width: 479px) {
	.box-content {
		max-width: 30%;
		margin-bottom: 15px;
	}
}
.border-box {
	text-align: center;
	display: inline-block;
	padding: 17px 30px;
	width: 90%;
	border: 1px solid #949292;
	margin-bottom: 10px;
}
.border-box:hover,
.border-box:active,
.border-box:focus {
	box-shadow: 2px 2px 2px #505050;
}
.border-box > a {
	display: block;
	width: 100%;
	height: 100%;
}
.border-box > a > p {
	color: #505050;
}
#carousel-what-we-do .carousel-control {
  width: 5%;
  background: none;
}
#carousel-what-we-do .carousel-control .fa-chevron-left,
#carousel-what-we-do .carousel-control .fa-chevron-right {
  position: absolute;
  top: 40%;
  z-index: 5;
  left: 0;
  color: #949292;
}
.carousel-indicators {
	bottom: -20px;
	left: 40%;
	width: 80%;
}
.carousel-indicators li {
	width: 5px;
	height: 5px;
	margin: 1px;
	border: 1px solid #505050;
	background-color: #505050;
}
.carousel-indicators .active {
	width: 7px;
	height: 7px;
	border-color: #22c488;
}
.text-gray {
	color: #505050;
}
.text-gray h1 {
	margin-top: 30px;
}

/*clients*/
#carousel-clients {
	margin-bottom: 10px;
}
#carousel-comments .carousel-control {
  left: -35px;
  width: 5%;
  background: none;
}
#carousel-comments .carousel-control.right {
  right: -20px;
  left: auto;
}
#carousel-comments .carousel-control .fa-chevron-left,
#carousel-comments .carousel-control .fa-chevron-right {
  position: absolute;
  top: 50%;
  z-index: 5;
  display: inline-block;
}
.comments.background-light-grey {
	font-family: TradeGothicLight;
	padding: 25px 30px;
	margin: 20px 0;
}
.media-left {
	padding-right: 20px;
}
.media img {
	height: 70px;
	width: 70px;
}
.social img {
	width: 47px;
	height: auto;
	display: inline;
	margin: 0 20px;
}
.media-heading a {
	color: #22c488;
}

/*Contact*/
#contact {
	margin-bottom: 10px;
}
.btn-orange {
	color: #ffffff;
	border-color: #fd9610;
	background-color: #fd9610;
}
.btn-orange:active,
.btn-orange:hover,
.btn-orange:focus {
	color: #ffffff;
	border-color: #db7400;
	background-color: #db7400;
}
.background-box {
	text-align: center;
	display: inline-block;
	padding: 17px 30px;
	width: 80%;
	margin-bottom: 10px;
	margin-top: 10px;
	background-color: #ffffff;
}
.background-box > img {
	margin-bottom: 10px;
}

/***** Search Result *****/
/*Search Form*/
.btn-search-results {
  color: #ffffff;
  background-color: #949292;
  border-color: #ccc;
}

/*Results*/
.box-search {
	display: inline-block;
	padding: 3px 10px;
	width: 100%;
	border: 1px solid #505050;
	margin-bottom: 10px;
}
.box-search:active,
.box-search:focus,
.box-search:hover {
	box-shadow: 2px 2px 2px #505050;
}
.box-search > a {
	color: #505050;
}
.box-search img {
	vertical-align: super;
	display: inline;
	width: 70px;
	margin-right: 10px;
}
.description {
	box-sizing: border-box;
	display: inline-block;
	width: 70%;
	width: -moz-calc(100% - 70px - 20px);
	width: -webkit-calc(100% - 70px - 20px);
	width: calc(100% - 70px - 20px);
}
.description h3 {
	border-bottom: 1px solid #505050;
}
.number-pin{
	position: relative;
    display: inline-block;
    height: 30px;
    width: 22px;
    float: left;
    background: url(<%= asset_path 'search/pin.png' %>);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center top;
    margin-right: 4px;
}
.number-pin span{
    position: absolute;
    left: 30%;
    top: 15%;
    font-size: 13px;
    font-weight: bold;
    color: #ffffff;
}
