/*** navbar-agendapro ***/
body {
	padding-top: 40px;
}
.navbar-agendapro {
	background-color: rgba(255,255,255,0);
	border-color: rgba(255,255,255,0);
	-webkit-transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	transition: all 0.5s ease-in-out;
}
.navbar-agendapro > .container .navbar-brand {
	margin-left: 0px;
	padding: 5px 5px;
}
.navbar-agendapro .navbar-text {
	color: #ffffff;
}
.navbar-agendapro .navbar-toggle {
	border-color: #ffffff;
	color: #ffffff;
	padding: 5px 8px;
}
.navbar-agendapro-white {
	background-color: #ffffff;
	border-color: #eeeeee;
	-webkit-transition: all 0.5s ease-in-out;
	-moz-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	transition: all 0.5s ease-in-out;
}
.navbar-agendapro-white .navbar-toggle {
	border-color: #ccc;
	color: #ccc;
}
.btn-orange {
	color: #ffffff;
	background-color: #fd9610;
	border-color: #db7400;
}
.btn-orange:hover,
.btn-orange:focus,
.btn-orange.focus,
.btn-orange:active,
.btn-orange.active {
  color: #ffffff;
  background-color: #db7400;
  border-color: #DF840D;
}

