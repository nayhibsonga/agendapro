body {
	padding-top: 45px;
	padding-bottom: 5px;
}

/*** navbar-agendapro ***/
.navbar-agendapro {
	background-color: #22c488;
	border-color: #1ba970;
}
.navbar-agendapro > .container .navbar-brand {
	height: 36px;
	color: #ffffff;
	line-height: inherit;
}
.navbar-agendapro .navbar-text {
	color: #ffffff;
	text-align: center;
	font-weight: bold;
}
.navbar-agendapro .navbar-toggle {
	border-color: #ffffff;
	color: #ffffff;
	padding: 5px 8px;
}

/*Overview*/
.box-search {
	display: inline-block;
	padding: 3px 10px;
	width: 100%;
	border: 1px solid #949292;
	margin-bottom: 10px;
}
.box-search:active,
.box-search:focus,
.box-search:hover {
	border-color: #22c488;
	background-color: #eeeeee;
}
.box-search > a {
	color: #505050;
	outline: 0;
}
.box-search > a:active,
.box-search > a:focus,
.box-search > a:hover {
	text-decoration: none;
}
.bloque-local h3 {
	border-bottom: 1px solid #505050;
}

/*Workflow*/
.hidden {
	display: none;
}
.company-logo {
	width: 49%;
	max-width: 200px;
	display: inline;
	vertical-align: bottom;
	padding: 15px;
}
.btn-wide {
	width: 49%;
	max-width: 200px;
	font-size: 18px;
	line-height: 0;
	margin-bottom: 20px;
}
.btn-green {
	color: #ffffff;
	background-color: #22c488;
	border-color: #1ba970;
}
.btn-green:hover,
.btn-green:focus,
.btn-green.focus,
.btn-green:active,
.btn-green.active {
  color: #ffffff;
  background-color: #1ba970;
  border-color: #00CA8A;
}
.btn-orange {
	color: #ffffff;
	background-color: #fd9610;
	border-color: #db7400;
}
.btn-orange:hover,
.btn-orange:focus,
.btn-orange.focus,
.btn-orange:active,
.btn-orange.active {
  color: #ffffff;
  background-color: #db7400;
  border-color: #DF840D;
}
.panel-agendapro {
  border-color: #1ba970;
}
.panel-agendapro > .panel-heading {
  color: #ffffff;
  text-align: center;
  background-color: #22c488;
  border-color: #1ba970;
}
.panel-agendapro > .panel-heading + .panel-collapse > .panel-body {
  border-top-color: #1ba970;
}
.panel-agendapro > .panel-footer + .panel-collapse > .panel-body {
  border-bottom-color: #1ba970;
}
.panel-white {
  border-bottom-color: #ddd;
}
.panel-white > .panel-heading,
.panel-white > .panel-heading > a {
  color: #333;
}
.panel-group .panel-heading + .panel-collapse > .panel-body,
.panel-group .panel-heading + .panel-collapse > .list-group {
  border-top: none;
}
.panel-title-caret:after {
	font-family: 'FontAwesome';
	content: "\f0d7";
	float: right;
	color: #22c488;
}
a.collapsed .panel-title-caret:after {
	font-family: 'FontAwesome';
	content: "\f0d9";
	float: right;
	color: #1ba970;
}
/*select_hour*/
.container-margin {
	margin-top: 20px;
	margin-bottom: 5px;
}
.btn-press,
.btn-press:hover,
.btn-press:active,
.btn-press:focus {
	color: #ffffff;
	background: #fd9610;
	outline: 0;
	-webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
	box-shadow: inset 0 3px 5px rgba(0, 0, 0, .125);
}
/*book*/
.pull-center > img {
	width: 45%;
	max-width: 200px;
}
.vertical-divider {
	border-right: 1px solid #949292;
	height: 60px;
	display: inline-block;
	vertical-align: middle;
	margin: 0 4px;
}
.background-grey {
	background-color: #949292;
	color: #ffffff;
	padding: 5px;
}
.background-grey h4 {
	margin-top: 30px;
}
.background-grey .table td {
	padding-left: 15px;
}
.big-btn {
	white-space: normal;
	width: 49%;
	font-size: 18px;
	line-height: 1.33;
	border-radius: 6px;
}
