#sessions-div{
    border-left: 1px solid #949292;
    border-right: 1px solid #949292;
}
.session-row{
    padding: 5px 5px 5px 5px;
    border-bottom: 1px solid #949292;
    margin-left: 0px !important;
    margin-right: 0px !important;
    color: #505050;
}

.used-session-row{
    padding: 5px 5px 5px 5px;
    border-bottom: 1px solid #949292;
    margin-left: 0px !important;
    margin-right: 0px !important;
    color: #505050;
    background: #E1E0E0;
}
.session-row-btns{
    text-align: right;
}

.session-btn{
    width: 45%;
}

.session-btn-book{
    color: #fff;
    border-color: #22c488;
    background-color: #22c488;
}

.session-btn-book:hover{
    color: #fff;
    border-color: #1ba970;
    background-color: #1ba970;
}

.session-btn-edit{
    color: #fff;
    border-color: #fd9610;
    background-color: #fd9610;
    margin-right: 5px;
}

.session-btn-edit:hover{
    color: #fff;
    border-color: #db7400;
    background-color: #db7400;
    margin-right: 5px;
}

.session-btn-delete{
    color: #fff;
    border-color: #fd633f;
    background-color: #fd633f;
}

.session-btn-delete:hover{
    color: #fff;
    border-color: #E55938;
    background-color: #E55938;
}

.session-btn-detail{
    color: #fff;
    border-color: #BAB8B8;
    background-color: #BAB8B8;
}

.session-btn-detail:hover{
    color: #fff;
    border-color: #9D9B9B;
    background-color: #9D9B9B;
}

.session-btn-validate{
    color: #fff;
    border-color: #0f91cf;
    background-color: #0f91cf;
}

.session-btn-validate:hover{
    color: #fff;
    border-color: #0b587b;
    background-color: #0b587b;
}