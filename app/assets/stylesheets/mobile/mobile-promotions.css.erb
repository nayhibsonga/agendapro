#economic-sectors-row{
	display: inline-block;
	white-space: nowrap;
	position: fixed;
	width: 100%;
	overflow-x: scroll;
	//-webkit-overflow-scrolling: touch;
	padding-top: 15px;
	padding-bottom: 18px;
	background-color: #fff;
	z-index: 1020;
	padding-right: 25px;
}

#promotions-div{
	margin-top: 61px;
}

.economic-sector-span{
	min-width: 100px;
}

.economic-sector-btn{
	display: inline-block;
	padding-left: 4px;
	padding-right: 4px;
	border: 1px solid #505050;
	border-radius: 3px;
	color: #505050;
	background-color: #fff;
	margin: 5px;
	min-width: 100px;
	text-align: center;
}

.economic-sector-btn.active{
	border: 1px solid #22c488;
	border-radius: 3px;
	color: #fff;
	background-color: #22c488;
}

.promo-box{
	width: 100%;
}

.promo-photo{
	text-align: center;
	background-color: #f0f0f0;
}

.promo-img{
	display: block;
	margin-left: auto;
	margin-right: auto;
}

.promo-title{

  padding-left: 5px;
  padding-top: 5px;
  padding-bottom: 4px;
  background-color: #B5B5B5;
  color: #505050;
  display: block;
  width: 100%;
  font-weight: bold;
  font-size: 14px;

}

.promo-company{
	text-align: left;
	font-size: 14px;
}

.promo-discount{
	text-align: right;
	font-size: 14px;
}

.promo-detail{
	height: 100%;
	width: 100%;
	padding: 5px;
	font-size: 14px;
}

.promo-description{
	margin: 5px;
	text-align: justify;
	padding: 4px;
	border: 1px solid #949292;
	border-radius: 3px;
	color: #949292;
	font-size: 14px;
}



.panel-group > .panel > .panel-heading > .panel-title.minus > a:after {
	/* Font symbol */
	color: #22c488;
	content: "\f056";
}
.panel-group > .panel > .panel-heading > .panel-title.minus.collapsed > a:after,
.panel-group > .panel > .panel-heading > .panel-title.minus > a.collapsed:after {
	/* Font symbol */
	color: #22c488;
	content: "\f055";
}

.panel-heading{
	padding-left: 0px !important;
	background-color: #fff !important;
}

#accordion .panel-title{
	border: none !important;
	background-color: #fff;
	font-size: 18px;
	color: #949292;
}

.panel-title a:hover{
	text-decoration: none !important;
}

.panel-title a{
	text-decoration: none !important;
}

#accordion .panel-default{
	border-top: none;
	border-left: none;
	border-right: none;
	padding-left: 15px;
}

.description-body p{
	color: #949292; 
	font-size: 14px !important;
}

.hours-title{
	color: #505050;
	font-size: 14px;
}

.discount-time{
	color: #505050;
	font-size: 14px;
	text-align: center;
}

.hours{
	color: #949292;
  	font-size: 11px;
  	text-align: left;
}

.day-title{
	font-weight: bold;
	font-size: 12px;
	text-align: left;
}

.day-discount{
	color: #22c488;
	font-size: 12px;
	text-align: center;
}

.promo-footer{
	background-color: #22c488;
	width: 100%;
	height: 100%;
}

#openHoursBtn{
	margin-top: 6px;
	margin-right: 6px;
	padding-left: 7px;
	padding-right: 7px;
	padding-top: 3px;
	padding-bottom: 3px;
	color: #22c488;
	background-color: #fff;
	border-radius: 5px;
	border: 1px solid #22c488;
	float: right;
}

#openHoursBtn:disabled{
	background-color: #d5d5d5;
	border-color: #d5d5d5;
}

#inputLocalizationDiv{
	width: 100%;
	position: fixed;
	bottom: 0px;
	left: 0px;
}

#inputLocalizationMarker{
	float: right;
	margin-right: 18px;
	margin-bottom: 8px;
}


#inputLocalizationRow{
	background-color: #22c488;
	width: 100%;
}

#inputLocalizationResults{
	height: 50px;
}

.inputLocalizationElement{
	margin: 10px;
}

.btn-search {
  color: #ffffff;
  background-color: #1ba970 !important;
  border-color: #ffffff;
}

#datepicker{
	background-color: #fff !important;
}