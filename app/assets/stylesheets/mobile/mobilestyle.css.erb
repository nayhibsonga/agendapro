@font-face {
	font-family: TradeGothic;
	src: url(<%= font_path('TradeGothicLTStd.otf') %>);
}
@font-face {
	font-family: TradeGothicCn18;
	src: url(<%= font_path('TradeGothicLTStd-Cn18.otf') %>);
}
@font-face {
	font-family: TradeGothicLight;
	src: url(<%= font_path('TradeGothicLTStd-Light.otf') %>);
}

html,
body {
	font-family: TradeGothicCn18, TradeGothic;
	background: #ffffff;
	font-size: 17px;
}
h1, h2, h3, h4, h5, h6,
.h1, .h2, .h3, .h4, .h5, .h6 {
	font-family: TradeGothicCn18;
	line-height: 1.1;
}
h1, .h1 {
  font-size: 2.22em;
}
h2, .h2 {
  font-size: 1.86em;
}
h3, .h3 {
  font-size: 1.51em;
}
h4, .h4 {
  font-size: 1.16em;
}
h5, .h5 {
  font-size: 0.92em;
}
h6, .h6 {
  font-size: 0.81em;
}

.pull-center {
	text-align: center;
	display: block;
	margin-left: auto;
	margin-right: auto;
}

/*** navbar-agendapro ***/
.navbar-agendapro {
	background-color: rgba(255,255,255,0);
	border-color: rgba(255,255,255,0);
}
.navbar-agendapro > .container .navbar-brand {
	margin-left: 0px;
	padding: 5px 5px;
}
.navbar-agendapro .navbar-text {
	color: #ffffff;
}
.close-menu i {
	border: #ffffff solid;
	border-radius: 100%;
	padding: 2px 4px;
}
.navbar-agendapro .navbar-toggle {
	border-color: #777;
	color: #333;
}
nav.menu a.menu-green {
	color: #00e99f;
}
nav.menu a.menu-green:hover,
nav.menu a.menu-green:focus {
	color: #ffffff;
	background-color: transparent;
}
nav.menu a.menu-orange {
	color: #fd9610;
}
nav.menu a.menu-orange:hover,
nav.menu a.menu-orange:focus {
	color: #ffffff;
}
body.smr-open {
	height: 100%;
	height: 100vh;
	overflow: hidden;
	-webkit-overflow-scrolling: hidden;
	width: 100%;
	position: fixed;
}
nav.menu li.divider {
	margin-top: 30px;
}
