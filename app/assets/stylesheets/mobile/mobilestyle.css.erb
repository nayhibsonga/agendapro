/*** alert ***/
.alert {
	position: absolute;
	margin-top: 50px;
	width: 100%;
	z-index: 10;
}

/*background*/
.background {
	background-repeat: no-repeat;
	background-size: cover;
	background-position: center center;
	color: #ffffff;
}
.background-grey {
	background-color: #eeeeee;
	padding: 30px 0;
	margin-top: 30px;
}
.background-light-grey {
	background-color: #f8f8f8;
	padding: 10px 0;
	margin-top: 30px;
}
.background-green {
	background-color: #00e99f;
	padding: 20px 0 10px;
	margin-top: 30px;
}
.overlay {
	background-color: rgba(87, 91, 104, 0.63);
}
.dark-overlay {
	background-color: rgba(0,0,0,0.5);
	padding: 20px 0;
}
.below-navbar {
	padding-top: 50px;
}

/*Search Text*/
/*h1.search-text {
	margin-top: 10%;
	margin-bottom: 20px;
}*/
.search-text {
	color: #ffffff;
	text-transform: uppercase;
	/*font-size: 73px;*/
	font-family: TradeGothicCn18;
	font-weight: 300;
}
.search-text-green {
	color: #00e99f;
}

/*Search Form*/
.image-pin {
	width: 22.07%;
	width:	20vw;
	max-width: 99px;
}
#search-form {
	margin-top: 10px;
	margin-bottom: 30px;
}
/*.form-container {
	left: -65px;
}*/
.btn-search {
  color: #333;
  background-color: #fff;
  border-color: #ccc;
}
.btn-search:hover,
.btn-search:focus,
.btn-search:active,
.btn-search.active {
  color: #818181;
  background-color: #ffffff;
  border-color: #adadad;
}
.btn-search:active,
.btn-search.active {
  background-image: none;
}
.form-transparent {
	background:rgba(255,255,255,0);
    border-color: rgba(255,255,255,1);
    color: #ffffff;
}
.form-transparent:-moz-placeholder {
  color: #ffffff;
}
.form-transparent::-moz-placeholder {
  color: #ffffff;
}
.form-transparent:-ms-input-placeholder {
  color: #ffffff;
}
.form-transparent::-webkit-input-placeholder {
  color: #ffffff;
}
.green-placeholder:-moz-placeholder {
  color: #818181;
}
.green-placeholder::-moz-placeholder {
  color: #818181;
}
.green-placeholder:-ms-input-placeholder {
  color: #818181;
}
.green-placeholder::-webkit-input-placeholder {
  color: #818181;
}

.clients {
	height: 68px;
	overflow: hidden;
}
.clients img {
	padding-left: 7px;
	padding-left: 2vw;
	padding-right: 7px;
	padding-right: 2vw;
}
.clients img:first-child {
	padding-left: 0;
}
.clients img:last-child {
	padding-right: 0;
}

/*Waht we do*/
.box-content {
	text-align: center;
	display: inline-block;
	padding: 17px 30px;
	width: 80%;
	border: 1px solid #ffffff;
}
@media (min-width: 479px) {
	.box-content {
		max-width: 147px;
	}
}
.box-content > p {
	margin: 9px 0 0;
}
.row-border {
	border-bottom: 1px solid #ddd;
	border-top: 1px solid #ddd;
	margin-bottom: 30px;
	width: 75.5%;
}
.box-border-right {
	border-right: 1px solid #ddd;
}
.box-border-left {
	border-left: 1px solid #ddd;
}
.border-box {
	padding: 10px 13px;
	margin: 5px 10px;
	border: 1px solid #ddd;
	border-radius: 5px;
	text-align: center;
	display: inline-table;
}
.border-box > p {
	padding-top: 9px;
	margin: 0px;
}
.text-gray {
	color: #767676;
}
.text-gray h1 {
	margin-top: 30px;
}
#margin {
	margin-top: 15px;
	margin-bottom: 20px;
}
.btn-img {
	width: 148px;
	height: 57px;
	background-color: #ffffff;
	color: #333;
	margin: 5px;
	font-size: 15px;
}
.btn-img > img {
	width: auto;
	height: 50%;
}

/*clients*/
.background-grey img {
	margin: 10px;
}
.media-heading {
	border-bottom: #767676 solid 1px;
}
.social {
	font-size: 16px;
	color: #ffffff;
	display: inline;
	margin: 0 20px;
}
.social > img {
	width: 47px;
	height: auto;
}
.social > span {
	margin-left: 10px;
}
.fa-green {
	color: #00e99f;
}
.social-facebook:hover {
	color: #3b5998;
}
.social-twitter:hover {
	color: #55acee;
}
.social-vimeo:hover {
	color: #4bf;
}

/*Contact*/
.btn-green {
	color: #00e99f;
	border-color: #00e99f;
}
.contact-footer {
	padding-top: 50px;
	padding-bottom: 10px;
}
.contact-footer img,
.contact-footer p {
	margin: 5px;
	white-space: pre-line;
}
.footer {
	padding-top: 10px;
}
.footer a {
	color: #ffffff;
}
.footer img {
	margin-top: -20px;
}
