/*Waht we do*/
.box-content {
	text-align: center;
	display: inline-block;
	padding: 17px 30px;
	width: 80%;
	border: 1px solid #ffffff;
	margin-bottom: 10px;
}
.box-content > p {
	margin: 9px 0 0;
}
@media (min-width: 479px) {
	.box-content {
		max-width: 30%;
		margin-bottom: 15px;
	}
}
.border-box {
	text-align: center;
	display: inline-block;
	padding: 17px 30px;
	width: 90%;
	border: 1px solid #bbbaba;
	margin-bottom: 10px;
}
.carousel-indicators {
	bottom: -20px;
	left: 40%;
	width: 80%;
}
.carousel-indicators li {
	width: 5px;
	height: 5px;
	margin: 1px;
	border: 1px solid #767676;
	background-color: #767676;
}
.carousel-indicators .active {
	width: 7px;
	height: 7px;
	border-color: #00e99f;
}
.text-gray {
	color: #767676;
}
.text-gray h1 {
	margin-top: 30px;
}

/*clients*/
#carousel-clients {
	margin-bottom: 10px;
}
.carousel-control {
  left: -35px;
  width: 5%;
}
.carousel-control.left {
  background: none;
}
.carousel-control.right {
  right: -20px;
  left: auto;
  background: none;
}
.carousel-control .fa-chevron-left,
.carousel-control .fa-chevron-right {
  position: absolute;
  top: 50%;
  z-index: 5;
  display: inline-block;
}
div.background-grey {
	font-family: TradeGothicLight;
	padding: 25px 30px;
	margin: 20px 0;
}
.media-left {
	padding-right: 20px;
}
.media img {
	height: 70px;
	width: 70px;
}
.social > img {
	width: 47px;
	height: auto;
	display: inline;
	margin: 0 20px;
}

/*Contact*/
.btn-green {
	color: #00e99f;
	border-color: #00e99f;
}
.contact-footer {
	padding-top: 50px;
	padding-bottom: 10px;
}
.contact-footer img,
.contact-footer p {
	margin: 5px;
	white-space: pre-line;
}
.footer {
	padding-top: 10px;
}
.footer a {
	color: #ffffff;
}
.footer img {
	margin-top: -20px;
}
