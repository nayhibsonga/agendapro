/*** alert ***/
.alert {
	position: absolute;
	margin-top: 50px;
	width: 100%;
	z-index: 10;
}

/*background*/
.background {
	background-repeat: no-repeat;
	background-size: cover;
	background-position: center center;
	color: #ffffff;
}
.background-grey {
	background-color: #eeeeee;
}
.background-light-grey {
	background-color: #f8f8f8;
	padding: 10px 0;
	margin-top: 30px;
}
.background-green {
	background-color: #00e99f;
	padding: 20px 0 10px;
	margin-top: 30px;
}
.overlay {
	background-color: rgba(87, 91, 104, 0.63);
}
.dark-overlay {
	background-color: rgba(0,0,0,0.5);
	padding: 20px 0;
}
.below-navbar {
	padding-top: 50px;
}

/*Search Text*/
/*h1.search-text {
	margin-top: 10%;
	margin-bottom: 20px;
}*/
.search-text {
	color: #ffffff;
	text-transform: uppercase;
	/*font-size: 73px;*/
	font-family: TradeGothicCn18;
	font-weight: 300;
}
.search-text-green {
	color: #00e99f;
}

/*Search Form*/
.image-pin {
	width: 22.07%;
	width:	20vw;
	max-width: 99px;
}
#search-form {
	margin-top: 10px;
	margin-bottom: 30px;
}
#search-form button,
#search-form input {
	font-size: 14px;
}
/*.form-container {
	left: -65px;
}*/
.btn-search {
  color: #333;
  background-color: #fff;
  border-color: #ccc;
}
.btn-search:hover,
.btn-search:focus,
.btn-search:active,
.btn-search.active {
  color: #818181;
  background-color: #ffffff;
  border-color: #adadad;
}
.btn-search:active,
.btn-search.active {
  background-image: none;
}
.form-transparent {
	background:rgba(255,255,255,0);
    border-color: rgba(255,255,255,1);
    color: #ffffff;
}
.form-transparent:-moz-placeholder {
  color: #ffffff;
}
.form-transparent::-moz-placeholder {
  color: #ffffff;
}
.form-transparent:-ms-input-placeholder {
  color: #ffffff;
}
.form-transparent::-webkit-input-placeholder {
  color: #ffffff;
}
.green-placeholder:-moz-placeholder {
  color: #818181;
}
.green-placeholder::-moz-placeholder {
  color: #818181;
}
.green-placeholder:-ms-input-placeholder {
  color: #818181;
}
.green-placeholder::-webkit-input-placeholder {
  color: #818181;
}

