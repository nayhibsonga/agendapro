/*** navbar-agendapro ***/
body {
	padding-top: 51px;
	color: #505050;
}
.navbar-agendapro {
	background-color: rgba(255,255,255,0);
	border-color: rgba(255,255,255,0);
}
.navbar-agendapro > .container .navbar-brand {
	margin-left: 0px;
	padding: 5px 5px;
}
.navbar-agendapro .navbar-text {
	margin-bottom: 0;
}
.navbar-agendapro .navbar-toggle {
	border-color: #ffffff;
	color: #ffffff;
	padding: 5px 8px;
	margin-bottom: 0;
	margin-right: 0;
}
.navbar-agendapro-white {
	background-color: #ffffff;
	border-color: #eeeeee;
}
.navbar-agendapro-white .navbar-toggle {
	border-color: #ccc;
	color: #ccc;
}
.form-control {
	font-size: 17px;
}

.btn-green {
	color: #ffffff;
	background-color: #22c488;
	border-color: #1ba970;
}
.btn-green:hover,
.btn-green:focus,
.btn-green.focus,
.btn-green:active,
.btn-green.active {
  color: #ffffff;
  background-color: #1ba970;
  border-color: #00CA8A;
}
.btn-orange {
	color: #ffffff;
	background-color: #fd9610;
	border-color: #db7400;
}
.btn-orange:hover,
.btn-orange:focus,
.btn-orange.focus,
.btn-orange:active,
.btn-orange.active {
  color: #ffffff;
  background-color: #db7400;
  border-color: #DF840D;
}
.btn-red {
	color: #ffffff;
	background-color: #fd633f;
	border-color: #e55938;
}
.btn-red:hover,
.btn-red:focus,
.btn-red.focus,
.btn-red:active,
.btn-red.active {
  color: #ffffff;
  background-color: #e55938;
  border-color: #CD4E31;
}

/*** Calendar **/
.background-green {
	background-color: #22c488;
	color: #ffffff;
	padding: 5px;
	margin-bottom: 10px;
}
.calendar-label {
	font-size: 16px;
	padding-right: 0;
	padding-top: 10px;
}
.calendar-date {
	padding-bottom: 10px;
	border-bottom: 1px solid #949292;
}
.btn-group > .btn {
	padding: 10px;
}
.btn-group > .btn:first-child,
.btn-group > .btn:last-child {
	padding: 10px 16px;
}
.calendar-date .btn-green {
	padding: 10px 13px;
}
#calendar thead {
	display: none;
}
.fc-agenda-slots td div {
	height: 60px !important;
}
.fc-event {
	border-radius: 4px;
}
.fc-event-inner {
	border-radius: 4px;
}
.fc-event-vert .fc-event-bg {
	display: none;
}
