/*** navbar-agendapro ***/
body {
	padding-top: 40px;
	color: #505050;
}
.navbar-agendapro {
	background-color: rgba(255,255,255,0);
	border-color: rgba(255,255,255,0);
	min-height: 40px;
}
.navbar-agendapro > .container .navbar-brand {
	margin-left: 0px;
	padding: 5px 5px;
	height: 40px;
}
.navbar-agendapro .navbar-text {
	margin-bottom: 0;
}
.navbar-agendapro .navbar-toggle {
	border-color: #ffffff;
	color: #ffffff;
	padding: 1px 6px;
	margin-top: 5px;
	margin-bottom: 0;
	margin-right: 0;
}
.navbar-agendapro-white {
	background-color: #ffffff;
	border-color: #eeeeee;
}
.navbar-agendapro-white .navbar-toggle {
	border-color: #ccc;
	color: #ccc;
}

.btn-green {
	color: #ffffff;
	background-color: #22c488;
	border-color: #1ba970;
}
.btn-green:hover,
.btn-green:focus,
.btn-green.focus,
.btn-green:active,
.btn-green.active {
  color: #ffffff;
  background-color: #1ba970;
  border-color: #00CA8A;
}
.btn-orange {
	color: #ffffff;
	background-color: #fd9610;
	border-color: #db7400;
}
.btn-orange:hover,
.btn-orange:focus,
.btn-orange.focus,
.btn-orange:active,
.btn-orange.active {
  color: #ffffff;
  background-color: #db7400;
  border-color: #DF840D;
}
.btn-red {
	color: #ffffff;
	background-color: #fd633f;
	border-color: #e55938;
}
.btn-red:hover,
.btn-red:focus,
.btn-red.focus,
.btn-red:active,
.btn-red.active {
  color: #ffffff;
  background-color: #e55938;
  border-color: #CD4E31;
}

/*** Calendar **/
.background-green {
	background-color: #22c488;
	color: #ffffff;
	padding: 10px 0;
}
.background-green > .row .col-xs-12 + .col-xs-12 {
	margin-top: 5px;
}
.background-green > .row > .col-xs-12 > .col-xs-4 {
	padding-right: 2px;
}
.background-green > .row > .col-xs-12 > .col-xs-8 {
	padding-left: 2px;
}
.calendar-label {
	font-weight: normal;
	vertical-align: -webkit-baseline-middle;
	font-size: 16px;
}
.calendar-date {
	padding-top: 10px;
	padding-bottom: 10px;
	border-bottom: 1px solid #949292;
}
#calendar thead {
	display: none;
}
.fc-event {
	border-radius: 4px;
}
.fc-event-inner {
	border-radius: 4px;
}

/*** view booking ***/
.booking {
	margin-top: 15px;
}
.booking tr > td:first-child {
	padding-left: 15px;
}
.booking tr > td:last-child {
	text-align: right;
	padding-right: 15px;
}
.modal-backdrop {
	position: fixed;
	height: 100% !important;
}
#cancelModal .modal-content {
	background-color: #22c488;
	color: #ffffff;
}
#cancelModal .modal-content > .modal-body {
	padding: 30px 15px;
}
