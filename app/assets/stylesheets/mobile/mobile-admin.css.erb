/*** view booking ***/
.booking {
	margin-top: 15px;
}
.booking tr > td:first-child {
	padding-left: 15px;
}
.booking tr > td:last-child {
	text-align: right;
	padding-right: 15px;
}
.modal-backdrop {
	position: fixed;
	height: 100% !important;
}
#cancelModal .modal-content {
	background-color: #22c488;
	color: #ffffff;
}
#cancelModal .modal-content > .modal-body {
	padding: 30px 15px;
}
#cancelModal .btn {
	padding: 4px 30px;
}
#cancelModal .btn-default {
	color: #22c488;
}

/*** Edit Booking ***/
.nav-pills > li {
	float: none;
	display: table-cell;
	width: 1%;
	text-align: center;
}
.nav-pills > li > a,
.nav-pills > li > a:hover,
.nav-pills > li > a:focus {
	color: #22c488;
	border: 1px solid #22c488;
}
.nav-pills > li.active > a,
.nav-pills > li.active > a:hover,
.nav-pills > li.active > a:focus {
	color: #ffffff;
	background-color: #22c488;
}

/*** Clients ***/
.background-grey {
	background: #eeeeee;
	padding: 10px 5px;
	margin-bottom: 10px;
}
#client_filter {
	margin-top: 10px;
	margin-bottom: 10px;
}
.list-inline > li {
	width: 100%;
	display: table-cell;
}
.list-inline > li:first-child {
	width: 8.33333333%;
}
.list-inline > li:last-child {
	width: 16.66666667%;
}
.date-select > .col-xs-3:not(label) {
	padding-left: 1px;
	padding-right: 1px;
}
.date-select > .col-xs-3:nth-child(2) {
	padding-left: 15px;
}
.date-select > .col-xs-3:last-child {
	padding-right: 15px;
}
