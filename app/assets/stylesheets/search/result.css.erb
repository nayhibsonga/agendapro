.panel-group > .panel > .panel-heading > .panel-title.minus > a:after {
    /* Font symbol */
    color: #22c488;
    content: "\f056";
}
.panel-group > .panel > .panel-heading > .panel-title.minus.collapsed > a:after,
.panel-group > .panel > .panel-heading > .panel-title.minus > a.collapsed:after {
    /* Font symbol */
    color: #22c488;
    content: "\f055";
}

.panel-group div{

    background-color: #fff;
}

.panel-group{
    border-radius: 6px;
}

.panel-group .panel{
    padding-left: 10px;
    border-top: 0px !important;
    border-left: 0px !important;
    border-right: 0px !important;
}

.panel-group .list-group{
    border: none !important;
    background-color: #fff;
}

.panel-group .list-group-item{
    border: none !important;
    background-color: #fff;
    font-size: 16px;
    padding: 0px !important;

}

.panel-group .panel-title{
    border: none !important;
    background-color: #fff;
    font-size: 18px;
    padding-top: 3px;

}

.panel-group .panel-title a:hover{
    text-decoration: none !important;
}

.service-detail{
    padding-top: 6px;
}

@media (max-width: 1199px) {
    .box-tabs > .nav-tabs > .center-li {
        width: 86px !important;
    }
}

@media (max-width: 991px) {
    .box-tabs > .nav-tabs > .center-li {
        width: 110px !important;
    }
}

/* Small devices (tablets, 768px and up) */
@media (max-width: 767px) {
    .box-tabs > .nav-tabs > .center-li {
        width: 110px !important;
    }
}
