#map {
    position: relative;
    padding-bottom: 18%; 
    // This is the aspect ratio
    overflow: hidden;
    height: 150px;
    box-shadow: 2px 2px 4px #ccc;
    border-top: 1px solid #ccc;
    border-left: 1px solid #ccc;
    max-height: 250px;
}

.header {
    background-color: #A6A6A6;
    padding: 5px 5px;
    font-weight: bold;
    font-size: 24.5px;
}

.box-tabs{
		width: 100%;
    //width: calc(100% - 20px);
