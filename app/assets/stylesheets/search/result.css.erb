/* Resultado Search */
#searchbar-result{
    margin-top: 18px;
}
.header-busqueda{
    height: 70px;
}
.google-maps-search {
    position: relative;
    padding-bottom: 75%; // This is the aspect ratio
    height: 0;
    overflow: hidden;
    box-shadow: 2px 2px 4px #ccc;
    border-top: 1px solid #ccc;
    border-left: 1px solid #ccc;
}

.number-pin
{
	display: block;
    height: 38px;
    width: 30px;
    float: left;
    //position: relative;
    //display: inline-block;


}

.pin-icon{
	position: relative;
	background: url(<%= asset_path 'pin1.png' %>);
	background-size: contain;
  background-repeat: no-repeat;
  background-position: center top;
}

.center-number{
	position: absolute;
 	top: 12px;
 	left: 17px;
 	text-align: center;
 	color: #505050 !important;
 	font-size: 12px;
}

.number-pin-gray{
    background: url(<%= asset_path 'pin2.png' %>);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center top;
}

.number-pin-green{
    background: url(<%= asset_path 'pin1.png' %>);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center top;
}

.number-pin span{
    display: block;
    width: 30px;
    padding-bottom: 18px !important;
    text-align: center;
    color: #fff;
    font-size: 14px;
    font-weight: bold;
    margin-top: 5px;
}

