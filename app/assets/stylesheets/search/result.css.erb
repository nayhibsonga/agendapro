@charset "UTF-8";

/* Resultado Search */
#searchbar-result{
    margin-top: 18px;
}
.header-busqueda{
    height: 70px;
}
.google-maps-search {
    position: relative;
    padding-bottom: 75%; // This is the aspect ratio
    height: 0;
    overflow: hidden;
    box-shadow: 2px 2px 4px #ccc;
    border-top: 1px solid #ccc;
    border-left: 1px solid #ccc;
}

.number-pin
{
	display: block;
    height: 38px;
    width: 22px;
    float: left;
    //position: relative;
    //display: inline-block;
    
    
}

.pin-icon{
	position: relative;
	background: url(<%= asset_path 'pin1.png' %>);
	background-size: contain;
  background-repeat: no-repeat;
  background-position: center top;
}

.center-number{
	position: absolute;
 	top: 10px;
 	left: 12px;
 	text-align: center;   	
 	color: #5d5d5d !important;
 	font-size: 12px;
}

.number-pin-gray{
    background: url(<%= asset_path 'pin2.png' %>);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center top;
}

.number-pin-green{
    background: url(<%= asset_path 'pin1.png' %>);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center top;
}

.number-pin span{
    display: block;
    width: 22px;
    padding-bottom: 20px !important;
    text-align: center;
    color: #fff;
    font-size: 14px;
    font-weight: bold;
    margin-top: 5px;
}

.pin-link{
	height: 60px !important;
}

.title-local{
    display: inline-block;
    font-size: 21px;
