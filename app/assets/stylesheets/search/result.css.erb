}

.pin {
    position: relative;
    width: 100%; /* for IE 6 */
    display: inline;
}

.pin span { 
    position: absolute;
    left: 0;
    //bottom: 8px;
    width: 22px;
    font-weight: bold;
    font-size: 12px;
    color: white;
    text-align: center;
}

