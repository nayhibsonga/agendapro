function loadServiceCommissions(service_id, location_id)
{

	$("#serviceCommissionsModalDiv").empty();

	$.getJSON('/service_commissions', {service_id: service_id}, function (service_commissions) {
	    $.each(service_commissions, function (key, service_commission) {

	    	if(service_commission.is_percent)
	    	{
		    	$("#serviceCommissionsModalDiv").append(
		    		'<div class="payment-form-div form-group">' +
		    		'<label class="col-xs-3 control-label">' + service_commission.provider_name + '</label>' +
		    		'<div class="col-xs-6"><input type="number" class="form-control service_commission_amount" provider_id="' + service_commission.provider_id + '" value="" /></div>' +
		    		'<div class="col-xs-6"><select class="form-control service_commission_is_percent" provider_id="' + service_commission.provider_id + '"><option value="1" selected>%</option><option value="0">$</option></select></div>' +
		    		'</div>'
		    	);
		    }
		    else
		    {
		    	$("#serviceCommissionsModalDiv").append(
		    		'<div class="payment-form-div form-group">' +
		    		'<label class="col-xs-3 control-label">' + service_commission.provider_name + '</label>' +
		    		'<div class="col-xs-6"><input type="number" class="form-control service_commission_amount" provider_id="' + service_commission.provider_id + '" value="" /></div>' +
		    		'<div class="col-xs-6"><select class="form-control service_commission_is_percent" provider_id="' + service_commission.provider_id + '"><option value="1">%</option><option value="0" selected>$</option></select></div>' +
		    		'</div>'
		    	);
		    }
	    });
	});

	$("#serviceCommissionsModal").modal('show');

}

