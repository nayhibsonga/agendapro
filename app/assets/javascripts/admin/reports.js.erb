$(function() {
  $('.tab_index.render').click(function(event) {
    $('#render_content').html('<p class="text-center"><i class="fa fa-spinner fa-spin fa-2x"></i></p>');
    var report = event.target.id.replace(/[0-9]/g, '');
    $.ajax({
        url: "/report_"+report+"/"+event.target.id.replace( /^\D+/g, ''),
        cache: false,
        success: function(html){
          $('#render_content').html(html);
        }
    });
  });
  $('select.report_select').change(function() {
    $('#status_details').html('<p class="text-center"><i class="fa fa-spinner fa-spin fa-2x"></i></p>');
    $.ajax({
        url: "/report_status_details/"+$('#status_id').val()+"/"+$('#time_range_id').val(),
        cache: false,
        success: function(html){
          $('#status_details').html(html);
        }
    });
  });
});
