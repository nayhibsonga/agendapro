                            <option value="<%= u.id %>" selected><%= u.unit %></option>
                          <% elsif u.unit == "Meses" %>
                            <option value="<%= u.id %>" selected><%= u.unit %></option>
                          <% else %>
                            <option value="<%= u.id %>"><%= u.unit %></option>
                          <% end %>
                        <% end %>
