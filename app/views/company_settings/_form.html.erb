                    <%= f.text_field :account_name, :class => 'form-control', placeholder: '' %>
                  </div>
                </div>
                <div class="form-group hidden account-info account-data">
                  <%= f.label :company_rut, 'Rut Titular', :class => 'col-md-3 control-label' %>
