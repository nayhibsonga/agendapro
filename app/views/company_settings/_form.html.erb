                </div>
                <div class="form-group hidden account-info account-data">
                  <%= f.label :account_type, 'Tipo de cuenta', :class => 'col-md-3 control-label' %>
                  <div class="col-md-5">
                    <select name="company_setting[account_type]" class="form-control">
