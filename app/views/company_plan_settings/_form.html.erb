<%= simple_form_for(@company_plan_setting) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_id %>
    <%= f.input :base_price %>
    <%= f.input :locations_multiplier %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
