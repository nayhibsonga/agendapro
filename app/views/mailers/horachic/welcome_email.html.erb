    <tr>
      <td style="padding-top:0; padding-right:18px; padding-bottom:18px; padding-left:18px;" class="mcnButtonBlockInner" valign="top" align="center">
        <table class="mcnButtonContentContainer" style="border-collapse: separate ! important;border-radius: 0px;background-color: #EE5E66;" border="0" cellpadding="0" cellspacing="0" width="100%">
          <tbody>
            <tr>
              <td style="font-family: Arial; font-size: 16px; padding: 20px;" class="mcnButtonContent" valign="middle" align="center">
                <%= link_to "   Ver Promociones   ", "http://www.horachic.cl/deals", class: "mcnButton", title: "   Ver Promociones   ", target: "_blank", style: "font-weight: bold;letter-spacing: normal;line-height: 100%;text-align: center;text-decoration: none;color: #FFFFFF;" %>
              </td>
            </tr>
          </tbody>
        </table>
      </td>
    </tr>
  </tbody>
