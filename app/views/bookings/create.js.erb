<% if @booking.errors.any? %>
  <%= render :partial=>'js_errors', :locals=> { :target=> @booking } %>
<% else %>
  alert('Guardado');
  $('#myModal').modal('hide');
  alert('id: <%= escape_javascript(@booking.id.to_s) %>');
  alert('title: "<%= escape_javascript(@booking.service.name) %>"');
  alert('start: "<%= escape_javascript(@booking.start.to_s) %>"');
  alert('end: "<%= escape_javascript(@booking.end.to_s) %>"');
  $('#calendar').fullCalendar('renderEvent',
			{
				id: <%= escape_javascript(@booking.id.to_s) %>,
				title: "<%= escape_javascript(@booking.service.name) %>",
				allDay: false,
				start: "<%= escape_javascript(@booking.start.to_s) %>",
				end: "<%= escape_javascript(@booking.end.to_s) %>",
				startEditable: true,
				color: '#1e8584'
			},
			true);
<% end %>