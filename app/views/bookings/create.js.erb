<% if @booking.errors.any? %>
  <%= render :partial=>'js_errors', :locals=> { :target=> @booking } %>
<% else %>
  alert('Guardado');
  $('#myModal').modal('hide');
  $('#calendar').fullCalendar('renderEvent',
			{
				id: <%= escape_javascript(@booking.id.to_s) %>,
				title: "<%= escape_javascript(@booking.service.name) %>",
				allDay: false,
				start: "<%= escape_javascript(@booking.start.to_s.split(/ /)[0]+'T'+@booking.start.to_s.split(/ /)[1]) %>",
				end: "<%= escape_javascript(@booking.end.to_s.split(/ /)[0]+'T'+@booking.end.to_s.split(/ /)[1]) %>",
				startEditable: true,
				color: '#1e8584'
			},
			true);
<% end %>