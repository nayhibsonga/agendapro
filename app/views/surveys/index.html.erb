<h1>Listing surveys</h1>

<table>
  <thead>
    <tr>
      <th>Quality</th>
      <th>Style</th>
      <th>Satifaction</th>
      <th>Comment</th>
      <th>Client</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @surveys.each do |survey| %>
      <tr>
        <td><%= survey.quality %></td>
        <td><%= survey.style %></td>
        <td><%= survey.satifaction %></td>
        <td><%= survey.comment %></td>
        <td><%= survey.client %></td>
        <td><%= link_to 'Show', survey %></td>
        <td><%= link_to 'Edit', edit_survey_path(survey) %></td>
        <td><%= link_to 'Destroy', survey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Survey', new_survey_path %>
