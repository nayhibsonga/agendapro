<%= simple_form_for(@survey) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :quality %>
    <%= f.input :style %>
    <%= f.input :satifaction %>
    <%= f.input :comment %>
    <%= f.association :client %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
