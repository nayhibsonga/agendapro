<%= simple_form_for(@employee_code) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :code %>
    <%= f.association :company %>
    <%= f.input :active %>
    <%= f.input :staff %>
    <%= f.input :cashier %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
