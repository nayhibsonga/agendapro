<li class="dropdown">
	<a href="" class="dropdown-toggle menu-green" data-toggle="dropdown"><%= current_user.email %> <i class="fa fa-chevron-down"></i></a>
	<ul class="dropdown-menu">
		<li>
		<% if current_user.company_id || current_user.role_id == Role.find_by(:name => 'Super Admin').id %>
			<%= link_to '<i class="fa fa-dashboard"></i> Panel Empresa'.html_safe, dashboard_path %>
		<% else %>
			<%= link_to '<i class="fa fa-building-o"></i> Agregar mi Compañía'.html_safe, add_company_path %>
		<% end %>
		</li>
		<li><%= link_to '<i class="fa fa-calendar"></i> Mi Agenda'.html_safe, my_agenda_path %></li>
		<li><%= link_to '<i class="fa fa-user"></i> Editar Perfil'.html_safe, edit_user_registration_path %></li>
		<li><%= link_to '<i class="fa fa-question"></i> Tutoriales'.html_safe, tutorials_path %></li>
		<li><%= link_to '<i class="fa fa-power-off"></i> Salir'.html_safe, destroy_user_session_path, method: :delete %></li>
	</ul>
</li>
