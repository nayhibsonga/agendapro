  <body>
    <%= render 'layouts/home_modals' %>
    <header>
      <nav class="navbar navbar-default navbar-fixed-top navbar-home" role="navigation" id="home-navbar">
        <div class="green-navbar">
          <div class="container">
            <div class="row">
              <div class="col-xs-6">
              Reserva en los mejores lugares ¡Ahora! <i class="fa fa-chevron-right" aria-hidden="true"></i> <strong><%= link_to "Buscador AgendaPro", search_path %></strong>
              </div>
              <div class="col-xs-6 text-right">
                <% if user_signed_in? %>
                  <%= render 'layouts/home_dropdown' %>
                <% else %>
                  <a href="#" data-toggle="modal" data-target="#signin-modal" class="upper-menu"><i class="fa fa-user"></i> Inicia Sesión</a>
                  <%= link_to '<i class="fa fa-pencil-square-o"></i> Regístrate</a>'.html_safe, new_user_registration_path(:company => true), :class => "upper-menu" %>
                <% end %>
              </div>
            </div>
          </div>
        </div>
        <div class="container">
          <!-- Brand and toggle get grouped for better mobile display -->
          <div class="row">
            <div class="col-xs-12">
              <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar">
                  <i class="fa fa-bars"></i>
                </button>
                <%= link_to localized_root_url(subdomain: false), class: 'navbar-brand' do %>
                  <%= image_tag 'logos/logo2.png', :alt => 'AgendaPro', :size => "127x43" %>
                <% end %>
              </div>
              <!-- Collect the nav links, forms, and other content for toggling -->
              <div class="collapse navbar-collapse" id="navbar">
                <ul class="nav navbar-nav navbar-right">
                  <li class="<%= 'active' if current_page?(home_path) %>"><%= link_to "Inicio", home_path %></li>
                  <li class="<%= 'active' if current_page?(features_path) %>"><%= link_to "Características", features_path %></li>
                  <li class="<%= 'active' if current_page?(view_plans_path) %>"><%= link_to "Planes", view_plans_path %></li>
                  <li class="<%= 'active' if current_page?(tutorials_path) %>"><%= link_to "Blog", 'http://blog.agendapro.co' %></li>
                  <li class="<%= 'active' if current_page?(contact_path) %>"><%= link_to "Contacto", contact_path %></li>
                  <%= render 'layouts/flag_dropdown' %>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </nav>
      <!-- start: Alerts -->
      <!-- end: Alerts -->
    </header>
    <!-- end: Header -->

    <!-- start: content -->
    <div class="main main-home">
      <%= yield %>
    </div>
    <!-- end: content -->

    <div class="home-footer">
      <div class="container">
        <div class="row">
          <div class="col-xs-12">
            <%= image_tag 'logos/logo1.png', :alt => 'AgendaPro', :size => "127x43", class: "" %>
          </div>
        </div>
        <div class="row">
          <div class="col-sm-4 padded-col">
            <div class="row">
              <div class="col-xs-12">
                <p><strong>Información Adicional:</strong></p>
              </div>
            </div>
            <div class="row">
              <div class="col-xs-12">
