<%= simple_form_for(@notification_email) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :company %>
    <%= f.input :emails %>
    <%= f.input :notification_type %>
    <%= f.input :receptor_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
