<div class="row">
  <div class="col-md-4 col-md-offset-1">
    <div class="well">
      <h4>Cantidad de Reservas</h4>
      <p><%= booking_count(@from, @to, @status_ids, @option, current_user.company_id, Location.accessible_by(current_ability).where(company_id: current_user.company_id, active: true).pluck(:id)) %></p>
    </div>
  </div>
  <div class="col-md-4 col-md-offset-2">
    <div class="well">
      <h4>Recaudación</h4>
      <p><%= number_to_currency(booking_revenue(@from, @to, @status_ids, @option, current_user.company_id, Location.accessible_by(current_ability).where(company_id: current_user.company_id, active: true).pluck(:id)), currency: ' $', separator: ',', delimeter: '.', precision: 0) %></p>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-6">
    <%= status_booking_by_hour(@from, @to, @status_ids, @option, current_user.company_id, Location.accessible_by(current_ability).where(company_id: current_user.company_id, active: true).pluck(:id)) %>
    <%= pie_chart status_booking_ranking3(@from, @to, @status_ids, @option, current_user.company_id, Location.accessible_by(current_ability).where(company_id: current_user.company_id, active: true).pluck(:id)), library: { title: "Ranking Servicio Utilizados" } %>
  </div>
  <div class="col-md-6">
    <%= pie_chart status_booking_by_day(@from, @to, @status_ids, @option, current_user.company_id, Location.accessible_by(current_ability).where(company_id: current_user.company_id, active: true).pluck(:id)), library: { title: "Reservas por Día de la Semana" } %>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <%= line_chart status_booking_by_hour(@from, @to, @status_ids, @option, current_user.company_id, Location.accessible_by(current_ability).where(company_id: current_user.company_id, active: true).pluck(:id)), discrete: true, library: {hAxis: {title: "Hora del Día" }, vAxis: {title: "# Reservas", format: "#"}, title: "Reservas por Hora por Día" } %>
  </div>
</div>
