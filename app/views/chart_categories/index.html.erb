<h1>Listing chart_categories</h1>

<table>
  <thead>
    <tr>
      <th>Chart Field</th>
      <th>Category</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @chart_categories.each do |chart_category| %>
      <tr>
        <td><%= chart_category.chart_field_id %></td>
        <td><%= chart_category.name %></td>
        <td><%= link_to 'Show', chart_category %></td>
        <td><%= link_to 'Edit', edit_chart_category_path(chart_category) %></td>
        <td><%= link_to 'Destroy', chart_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attribute category', new_chart_category_path %>
