<% content_for :scripts do %>
<script type="text/javascript">
  $(function() {
    $('.pricing-title').click(function (e) {
      var target = $(e.target).data('target');

      // Change Active Title
      $('.pricing-title.active').removeClass('active');
      $(this).addClass('active');

      // Hide pricing-col
      $('.pricing-col.active').removeClass('active');

      // Show pricing-col
      $('.pricing-col[data-plan="' + target + '"]').addClass('active');
    });

    var header = $('.pricing-header');
    var headerY = header.offset().top - 60/* Navbar height */;
    var isFloating = false;
    var $w = $(window);
    $w.scroll(function() {
      var scrollTop = $w.scrollTop();
      var shouldBeFloating = scrollTop > headerY;
      if (shouldBeFloating && !isFloating) {
        header.addClass("floating-header");
        $('.fill').show();
        isFloating = true;
      } else if (!shouldBeFloating && isFloating){
        header.removeClass("floating-header");
        $('.fill').hide();
        isFloating = false;
      };
    });

    $('[data-toggle="tooltip"]').tooltip();
  });
</script>
<% end %>
